#Two pointer
class Solution:
    def search(self, nums: List[int], target: int) -> int: 
        if len(nums)==0:
            return -1
        i=0
        j=len(nums)-1
        while i<=j:
            if target==nums[i]:
                return i
            elif target==nums[j]:
                return j
            elif target>nums[i]:
                i+=1    
            j-=1
        return -1
        
