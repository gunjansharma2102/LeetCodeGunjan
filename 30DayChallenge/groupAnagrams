class Solution:
    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:
        outputHashMap={}
        outputList=[]
        
        for i in range(len(strs)):
            listAlphabets="".join(sorted(strs[i]))
            if listAlphabets in outputHashMap.keys():
                outputHashMap[listAlphabets].append(strs[i])
            else:
                outputHashMap.update({listAlphabets:[strs[i]]})
            
        for value in outputHashMap.values():
            outputList.append(value)
        return outputList
            
            
        
        
        
                    
                
            
        
        
